{
  "name": "a2a-trace",
  "version": "0.1.0",
  "description": "A2A Agent Telemetry CLI - Monitor and trace multi-agent systems using OpenTelemetry with integrated observability stack",
  "keywords": [
    "opentelemetry",
    "telemetry",
    "tracing",
    "monitoring",
    "observability",
    "agents",
    "multi-agent",
    "prometheus",
    "loki",
    "tempo",
    "grafana",
    "distributed-tracing",
    "metrics",
    "logs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/A2ATrace/A2ATrace.git",
    "directory": "cli"
  },
  "homepage": "https://github.com/A2ATrace/A2ATrace#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "bin": {
    "a2a": "./dist/index.js"
  },
  "scripts": {
    "dev": "node --import ./register-ts-node.mjs src/index.ts",
    "build": "tsc -b",
    "start": "node dist/index.js",
    "frontend": "cd ../client && npm run build && rm -rf ../cli/client-dist && cp -r dist ../cli/client-dist"
  },
  "files": [
    "dist",
    "client-dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.206.0",
    "@opentelemetry/exporter-prometheus": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-logs": "^0.206.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "find-open-port": "^2.0.3",
    "fs-extra": "^11.3.2",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.7.2",
    "@types/prompts": "^2.4.9",
    "@types/ws": "^8.18.1",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
